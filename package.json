{
  "name": "template-react",
  "private": true,
  "version": "0.0.0",
  "description": "vite + react",
  "engines": {
    "node": ">=18",
    "vscode": "^1.75.0"
  },
  "publisher": "tomjs-test",
  "main": "dist/extension/index.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "hello-world.showHelloWorld",
        "title": "Hello World: Show"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "business-logic-code-generation-view",
          "title": "VSCode extension demo",
          "icon": "extension/assets/blcg_icon.svg"
        }
      ]
    },
    "views": {
      "business-logic-code-generation-view": [
        {
          "type": "webview",
          "id": "blcg-webview",
          "name": "demo page"
        }
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.65.0",
    "@vscode-elements/elements": "^1.9.0",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "ali-oss": "^6.x",
    "cec-client-server": "^1.3.2",
    "html-modifier": "^1.2.0",
    "openai": "^4.77.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "tailwindcss": "^3.4.17",
    "vscode": "^1.1.37"
  },
  "devDependencies": {
    "@tomjs/tsconfig": "^1.7.1",
    "@tomjs/vite-plugin-vscode": "^3.2.1",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/vscode": "^1.93.0",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "vite": "^6.0.3"
  }
}
